@using RepublicaEmpleos.Infrastructure.AppSettingsModels
@using Microsoft.Extensions.Options

@inject IOptions<ScriptTags> OptionsScriptTags
<!-- Open Graph data -->
@if (!string.IsNullOrWhiteSpace(OptionsScriptTags.Value?.FacebookOpenGraph))
{
    <meta property="fb:app_id" content="@OptionsScriptTags.Value.FacebookOpenGraph">
}
<meta property="og:title" content="RepublicaEmpleos - Busca tu Empleo aqui" />
<meta property="og:type" content="article" />
<meta property="og:description" content="Necesito poner algo aqui y no se que JAJAJA" />
<meta property="og:site_name" content="Republica Dominicana" />