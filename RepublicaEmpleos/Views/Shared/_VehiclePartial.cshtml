@model List<RepublicaEmpleos.DTO.Vehicle>

@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager

    <table class="table">
        <thead>
            <tr>
                <th>
                    Matricula
                </th>
                <th>
                    Tipo
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Matricula)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.VehicleTypeId)
                    </td>
                    <td>
                        <a asp-controller="vehicles" asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-controller="vehicles" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>