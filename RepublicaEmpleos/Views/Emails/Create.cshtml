@model RepublicaEmpleos.Email

@{
    ViewData["Title"] = "Create";
}
<div class="row">
    <div class="col-xl-12 order-xl-1">
        <div class="card card-profile shadow ct-example">
            <div class="col-md-4">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input asp-for="Id" hidden readonly />
                    <div class="form-group">
                        <label asp-for="Description" class="control-label"></label>
                        <input asp-for="Description" class="form-control" />
                        <span asp-validation-for="Description" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ProfileId" class="control-label"></label>
                        <input asp-for="ProfileId" class="form-control" readonly value="@ViewBag.ProfileId" />
                        <span asp-validation-for="ProfileId" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            <a asp-action="fullprofile" asp-controller="Home">Back to List</a>
        </div>
    </div>
</div>